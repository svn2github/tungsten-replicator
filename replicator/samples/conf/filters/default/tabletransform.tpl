# Transforms table names that match the from_regex into the to_regex.  
replicator.filter.tabletransform=com.continuent.tungsten.replicator.filter.DatabaseTransformFilter
replicator.filter.tabletransform.transformTables=true
# The filter can transform up to 3 table expressions
replicator.filter.tabletransform.from_regex1=foo
replicator.filter.tabletransform.to_regex1=bar
