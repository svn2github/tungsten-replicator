# Configuration file for release-replicator.sh script

# Base directory where we'll checkout the source code
BUILD_DIR=build/release

# SVN URLs of the required modules
# Replicator URL
TREP_SVN_URL=http://tungsten-replicator.googlecode.com/svn/trunk/replicator
# FSM URL. 
TFSM_SVN_URL=http://tungsten-replicator.googlecode.com/svn/trunk/fsm
# Commons URL.  
TCOM_SVN_URL=http://tungsten-replicator.googlecode.com/svn/trunk/commons
# Bristlecone URL. 
BRI_SVN_URL=https://bristlecone.svn.sourceforge.net/svnroot/bristlecone/trunk/bristlecone
# Tools URL (in community builder for now)
EXTRA_SVN_URL=https://tungsten.svn.sourceforge.net/svnroot/tungsten/branches/tungsten-2.0/community/extra
# Enterprise builder (temporary)
EBLD_SVN_URL=svn+ssh://forge.continuent.org/svnroot/tungsten/branches/tungsten-2.0/enterprise-builder

# Version name of the release to build 
VERSION=2.0-rc-17

# Set to 1 to use existing code (found in $BUILD_DIR)
SKIP_CHECKOUT=0

# Set to 1 to skip the build.
SKIP_BUILD=0

# Set to 1 to skip source generation. 
SKIP_SOURCE=1

ENTERPRISE_BUILD=1
